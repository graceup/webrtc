<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
	
	<!-- ===================WebSocket====================== -->
	<servlet>
		<servlet-name>WebSocket</servlet-name>
		<servlet-class>com.graceup.webrtc.control.WebrtcWebSocket</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WebSocket</servlet-name>
		<url-pattern>/websocket</url-pattern>
	</servlet-mapping>
	
  <servlet>
    <servlet-name>OnlineUserControl</servlet-name>
    <servlet-class>com.graceup.webrtc.control.OnlineUserControl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>OnlineUserControl</servlet-name>
    <url-pattern>/onlineUserControl</url-pattern>
  </servlet-mapping>
  
  
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
